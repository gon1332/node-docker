<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
<div class="row">
    <div class="col-sm-8">
        <div class="jumbotron">
            <h1>Notes on Everything</h1>
            <ul>
                <% quotes.forEach(function(quote) { %>
                    <li><%= quote %></li>
                <% }); %>
            </ul>
        </div>
    </div>

    <div class="col-sm-4">
        <div class="well">
            <img src="<%= about_image %>" alt="How you doin?"/>
        </div>
    </div>

</div>
</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>
