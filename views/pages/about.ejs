<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
<div class="row">
    <div class="col-sm-8">
        <div class="jumbotron">
            <h1>Notes on Everything</h1>
            <ul>
                <% quotes.forEach(function(quote) { %>
                    <li><%= quote %></li>
                <% }); %>
            </ul>
        </div>
    </div>

    <div class="col-sm-4">
        <div class="well">
            <img src="https://i.pinimg.com/originals/6c/1c/5a/6c1c5a76db0dd7f80075be0f022319df.gif" alt="How you doin?"/>
        </div>
    </div>

</div>
</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>
